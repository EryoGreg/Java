<%@ include file="header.jsp" %><!DOCTYPE html><%@ include file="language.jsp" %><head>    <title>Comptes</title></head><c:if test="${param.lang != null}">    <fmt:setLocale value="${param.lang}" scope="session"/></c:if><body><h1>Bonjour, <c:out value="${client.prenom}"/> <c:out value="${client.nom}"/></h1><div >    <form id="transactionsForm" action="/maketransactions" method="post">        <div>            <div>                <select  class= "form-control" name="from" form="transactionsForm">                    <option value="" disabled selected>Choose your option</option>                    <c:forEach items="${client.comptes}" var="item">                        <option value="<c:out value="${item.id}"/>"><c:out value="${item.libelle}"/></option>                    </c:forEach>                </select>                <label><fmt:message key = "select_account"/></label>            </div>            <div>                <input name="to" placeholder="Account to be credited" class="form-control" id="account_credited" type="number" step="0.01">                <label for="account_credited"><fmt:message key = "enter_number"/></label>            </div>            <div>                <input name="amount" placeholder="Amount" class="form-control" id="amount_money" type="number" step="0.01">                <label for="amount_money"><fmt:message key = "amount"/></label>            </div>            <div>                <input name="libelle" placeholder="Libelle" class="form-control"id="lib" type="text">                <label for="lib"><fmt:message key = "libelle_reason"/></label>            </div>        </div>        <button class="btn btn-danger" type="submit" name="action">            <i><fmt:message key = "send"/></i>        </button>    </form></div><%@ include file="footer.jsp" %></body></html>