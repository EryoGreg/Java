<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.UtilisateurEntity" table="Utilisateur" schema="Java_Banque">
        <id name="id">
            <column name="Id" sql-type="int(11)"/>
        </id>
        <property name="nom">
            <column name="nom" sql-type="varchar(255)"/>
        </property>
        <property name="prenom">
            <column name="prenom" sql-type="varchar(255)"/>
        </property>
        <property name="email">
            <column name="email" sql-type="varchar(255)"/>
        </property>
        <property name="login">
            <column name="login" sql-type="varchar(255)"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(255)"/>
        </property>
        <property name="dateOfBirth">
            <column name="date_of_birth" sql-type="date"/>
        </property>
        <property name="address">
            <column name="address" sql-type="varchar(255)"/>
        </property>
        <property name="phone">
            <column name="phone" sql-type="varchar(255)"/>
        </property>
    </class>
</hibernate-mapping>