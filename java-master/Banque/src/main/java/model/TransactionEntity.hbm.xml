<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.TransactionEntity" table="Transaction" schema="Java_Banque">
        <id name="id">
            <column name="Id" sql-type="int(11)"/>
        </id>
        <property name="montant">
            <column name="montant" sql-type="float" precision="-1"/>
        </property>
        <property name="date">
            <column name="date" sql-type="date"/>
        </property>
        <property name="libelle">
            <column name="libelle" sql-type="varchar(255)"/>
        </property>
    </class>
</hibernate-mapping>